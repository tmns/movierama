{
  "name": "movierama",
  "version": "1.0.0",
  "description": "A movie catalog using the MDB JSON API",
  "main": "index.js",
  "scripts": {
    "dev": "sed -i 's/index.test.js/index.js/g' webpack.config.js && webpack --mode development",
    "build": "sed -i 's/index.test.js/index.js/g' webpack.config.js && webpack --mode production",
    "start": "sed -i 's/index.test.js/index.js/g' webpack.config.js && webpack-dev-server --mode development --open",
    "test": "sed -i 's/index.js/index.test.js/g' webpack.config.js && webpack-dev-server --mode development --open"
  },
  "author": "tmns <tmn5@pm.me>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4"
  }
}
